<html>
	<head>
		<meta charset="UTF-8">
		<title>background for wasavi</title>
		<style id="wasavi_global_styles"></style>
		<script src="lib/kosian/init.js"></script>
		<script src="lib/init.js"></script>
	</head>
	<body>
		<textarea id="clipboard-buffer"></textarea>
		<!--partial copy of wasavi.html start -->
		<div id="wasavi_container">
			<input id="wasavi_focus_holder" type="password">
			<div id="wasavi_editor"></div>
			<div id="wasavi_textwidth_guide"></div>
			<div id="wasavi_cursor_line"></div>
			<div id="wasavi_cursor_column"></div>
			<div id="wasavi_console_container">
				<div id="wasavi_console"></div>
			</div>
			<div id="wasavi_footer">
				<div id="wasavi_footer_status_container">
					<div id="wasavi_footer_file_indicator">#</div>
					<div id="wasavi_footer_prefix_indicator">#</div>
				</div>
				<div id="wasavi_footer_input_container">
					<div id="wasavi_footer_input_indicator">#</div>
					<input id="wasavi_footer_input" type="text" spellcheck="false" value="#">
				</div>
				<div id="wasavi_footer_notifier"></div>
			</div>
			<span id="wasavi_singleline_scaler"></div>
			<div id="wasavi_command_cursor"><span id="wasavi_command_cursor_inner"></span></div>
			<div id="wasavi_cover" tabindex="0">
				<span id="wasavi_cover_button">Press ^C to cancel</span>
			</div>
		</div>
		<!--partial copy of wasavi.html end -->
	</body>
</html>
